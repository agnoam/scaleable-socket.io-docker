{
  "name": "node-mini-app",
  "version": "2.4.0",
  "description": "extreamly minimal node js server (express, ts, jest, db)",
  "main": "dist/server.js",
  "scripts": {
    "prebuild": "eslint --fix",
    "build": "tsc",
    "heroku-postbuild": "webpack",
    "production": "node dist/server.bundle.js",
    "docker:production": "docker-compose up",
    "prestart": "npm run build",
    "start": "ts-node src/server.ts",
    "dev": "nodemon --inspect src/server.ts",
    "test": "jest"
  },
  "author": "Noam Aghai",
  "license": "ISC",
  "dependencies": {
    "body-parser": "1.18.3",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "elasticsearch": "^16.7.2",
    "express": "^4.16.2",
    "md5": "^2.2.1",
    "redis": "^3.1.2",
    "socket.io": "^4.0.1",
    "socket.io-redis": "^6.1.0"
  },
  "devDependencies": {
    "@types/elasticsearch": "^5.0.37",
    "@types/express": "4.16.1",
    "@types/jest": "24.0.12",
    "@types/md5": "^2.2.0",
    "@types/node": "12.0.0",
    "@types/redis": "^2.8.28",
    "@types/socket.io": "^2.1.13",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "eslint": "^7.25.0",
    "jest": "24.7.1",
    "nodemon": "^2.0.2",
    "socket.io-client": "^4.0.1",
    "ts-jest": "24.0.2",
    "ts-loader": "6.0.4",
    "ts-node": "^8.5.2",
    "tslint": "5.16.0",
    "typescript": "^3.9.4",
    "webpack": "4.28.4",
    "webpack-cli": "3.3.2",
    "webpack-node-externals": "1.7.2"
  }
